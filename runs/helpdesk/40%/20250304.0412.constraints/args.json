{
    "seed": 42,
    "epochs": 100,
    "learning_rate": 0.0001,
    "dataset": "helpdesk/40%",
    "model": {
        "embedding": 50,
        "hidden_size": 32,
        "num_layers": 2,
        "dropout": 0.2
    },
    "constraints": [
        "Alternate Succession[Assign seriousness, Resolve ticket]"
    ],
    "constraints_multiplier": 1.0
}
